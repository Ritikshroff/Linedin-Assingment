{
  "compilerOptions": {
     "outDir": "build", 
    "allowJs": true,
    "checkJs": false,
    "jsx": "react-jsx",
    "isolatedModules": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noEmit": false,
  },
  "include": ["src"]
}
